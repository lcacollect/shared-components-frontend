{
  "name": "@lcacollect/components",
  "version": "1.2.3",
  "description": "Shared component library for LCAcollect",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "vitest run",
    "codegen": "graphql-codegen --config codegen.yml",
    "build": "tsc -p tsconfig.json && tsc -p tsconfig.cjs.json",
    "build:watch": "npm run postbuild && tsc --watch",
    "postbuild": "copyfiles --flat ./src/assets/*.{png,svg} ./dist/esm/assets/ && copyfiles --flat ./src/assets/*.{png,svg} ./dist/cjs/assets/",
    "lint": "eslint src/**/*.{tsx,ts}",
    "lint:fix": "eslint --fix src/**/*.{tsx,ts}",
    "format:fix": "prettier --write src/",
    "format": "prettier --check src/",
    "fix": "npm run lint:fix && npm run format:fix"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lcacollect/shared-components-frontend.git"
  },
  "keywords": [],
  "author": "",
  "license": "Apache-2.0",
  "devDependencies": {
    "@apollo/client": "^3.7.2",
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/introspection": "^2.2.1",
    "@graphql-codegen/typescript-operations": "^2.4.2",
    "@graphql-codegen/typescript-react-apollo": "^3.2.16",
    "@graphql-codegen/typescript-resolvers": "^2.7.2",
    "@graphql-tools/mock": "^8.7.14",
    "@graphql-tools/schema": "^9.0.12",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@tsconfig/create-react-app": "^1.0.2",
    "@types/node": "^18.8.5",
    "@types/react": "^18.0.21",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "@vitejs/plugin-react": "^3.0.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest-environment-jsdom": "^29.1.2",
    "pre-commit": "^1.2.2",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4",
    "vitest": "^0.25.8"
  },
  "peerDependencies": {
    "@apollo/react-hooks": "^4.0.0",
    "@azure/msal-browser": "^2.30.0",
    "@azure/msal-react": "^1.4.9",
    "@emotion/styled": "^11.10.4",
    "@fontsource/roboto": "^4.5.8",
    "@mui/icons-material": "^5.10.9",
    "@mui/material": "^5.10.9",
    "@mui/x-data-grid-pro": "^5.17.7",
    "react": "^18.2.0",
    "react-router-dom": "^6.4.2",
    "semver": "^7.3.8"
  },
  "pre-commit": [
    "lint",
    "format"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.+(ts|tsx)": [
      "eslint --fix src/**/*.{tsx,ts}",
      "prettier --write",
      "git add"
    ],
    "*.+(json|css|md)": [
      "prettier --write",
      "git add"
    ]
  }
}
